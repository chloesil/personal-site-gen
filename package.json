{
  "name": "sirotkinco",
  "version": "0.0.1",
  "license": "MIT",
  "repository": "git://github.com/sashasirotkin/sirotkinco",
  "description": "Static site generator for Sasha Sirotkin's personal site",
  "dependencies": {
    "aws-sdk": "2.6.4",
    "bluebird": "3.4.6",
    "ejs": "2.5.2",
    "express": "4.14.0",
    "fs-extra": "0.30.0",
    "gray-matter": "2.0.2",
    "mime": "1.3.4",
    "recursive-readdir": "2.1.0",
    "showdown": "1.4.3"
  },
  "devDependencies": {
    "eslint": "3.6.0",
    "eslint-plugin-import": "1.16.0",
    "eslint-plugin-jsx-a11y": "2.2.2",
    "eslint-plugin-react": "6.3.0",
    "eslint-config-airbnb": "12.0.0"
  },
  "keywords": [
    "static",
    "file",
    "site",
    "website",
    "blog",
    "markdown"
  ],
  "scripts": {
    "watch": "nodemon -e md,js,ejs,css --watch src -x \"node tools/make | node tools/server\"",
    "make": "node tools/make"
  }
}
